{
    "name": "mezzio/mezzio-laminasrouter",
    "description": "laminas-mvc router support for Mezzio",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "mezzio",
        "http",
        "middleware",
        "psr",
        "psr-7"
    ],
    "homepage": "https://mezzio.dev",
    "support": {
        "docs": "https://docs.mezzio.dev/mezzio-laminasrouter/",
        "issues": "https://github.com/mezzio/mezzio-laminasrouter/issues",
        "source": "https://github.com/mezzio/mezzio-laminasrouter",
        "rss": "https://github.com/mezzio/mezzio-laminasrouter/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev",
            "dev-develop": "2.2.x-dev"
        }
    },
    "require": {
        "php": "^5.6 || ^7.0",
        "fig/http-message-util": "^1.1.2",
        "laminas/laminas-psr7bridge": "^0.2.2 || ^1.0.0",
        "laminas/laminas-router": "^3.0.2",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "mezzio/mezzio-router": "^2.1",
        "psr/http-message": "^1.0.1"
    },
    "require-dev": {
        "laminas/laminas-coding-standard": "~1.0.0",
        "laminas/laminas-i18n": "^2.7.3",
        "malukenho/docheader": "^0.1.5",
        "phpunit/phpunit": "^5.7.23 || ^6.4.3"
    },
    "autoload": {
        "psr-4": {
            "Mezzio\\Router\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MezzioTest\\Router\\": "test/"
        }
    },
    "scripts": {
        "check": [
            "@license-check",
            "@cs-check",
            "@test"
        ],
        "cs-check": "phpcs",
        "cs-fix": "phpcbf",
        "test": "phpunit --colors=always",
        "test-coverage": "phpunit --colors=always --coverage-clover clover.xml",
        "upload-coverage": "coveralls -v",
        "license-check": "docheader check src/ test/"
    },
    "replace": {
        "zendframework/zend-expressive-zendrouter": "self.version"
    }
}
